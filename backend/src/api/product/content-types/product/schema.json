{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "active": {
      "type": "boolean"
    },
    "price": {
      "type": "decimal"
    },
    "origin": {
      "type": "enumeration",
      "enum": [
        "Usaquén",
        "Chapinero",
        "Santa Fe",
        "San Cristóbal",
        "Usme",
        "Tunjuelito",
        "Bosa",
        "Kennedy",
        "Fontibón",
        "Engativá",
        "Suba",
        "Barrios Unidos",
        "Teusaquillo",
        "Los Mártires",
        "Antonio Nariño",
        "Puente Aranda",
        "La Candelaria",
        "Rafael Uribe Uribe",
        "Ciudad Bolívar",
        "Sumapaz"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "stock": {
      "type": "biginteger"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resena.resena"
    },
    "offer": {
      "type": "boolean"
    },
    "priceOffer": {
      "type": "decimal"
    },
    "user_actions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-action.user-action",
      "mappedBy": "product"
    }
  }
}
